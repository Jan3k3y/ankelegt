{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "channels": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "pattern": "^Anke([A-Z]+[a-z]*)+"
          },
          "description": {
            "type": "string"
          },
          "alts": {
            "type": "array",
            "items": {
              "type": "string",
              "pattern": "^Anke([A-Z]+[a-z]*)+"
            }
          }
        },
        "required": [
          "name",
          "description"
        ],
        "additionalProperties": false
      }
    }
  },
  "required": [
    "channels"
  ],
  "additionalProperties": false
}